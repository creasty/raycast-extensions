{
  "name": "slack-messages",
  "title": "Slack Messages",
  "description": "Control your Slack messages with Raycast!",
  "icon": "extension_icon.png",
  "author": "mattisssa",
  "license": "MIT",
  "commands": [
    {
      "name": "conversations",
      "title": "Unread Messages",
      "subtitle": "Slack",
      "description": "Manage unread messages",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "accessToken",
      "type": "password",
      "required": true,
      "title": "Access Token",
      "description": "Your Slack Access Token",
      "placeholder": "Enter your Slack access token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.0",
    "@slack/events-api": "^3.0.1",
    "@slack/web-api": "^6.4.0",
    "date-fns": "^2.25.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}